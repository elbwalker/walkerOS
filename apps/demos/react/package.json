{
  "name": "walkeros-demo-react",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsup && vite build",
    "test": "jest --colors",
    "test:watch": "jest --watchAll --colors",
    "preview": "vite preview",
    "start": "vite --host",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
