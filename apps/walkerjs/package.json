{
  "name": "@walkeros/walker.js",
  "version": "2.0.1",
  "description": "Ready-to-use walkerOS bundle with browser source, collector, and dataLayer support",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "browser": "./dist/walker.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/walker.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/walker.js"
    },
    "./es5": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.es5.js"
    },
    "./src/schemas": "./src/schemas.ts",
    "./src/dev": "./src/dev.ts"
  },
  "files": [
    "dist/**",
    "src/schemas.ts",
    "src/dev.ts",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest --colors --coverage",
    "dev": "jest --watchAll --colors",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf coverage",
    "preview": "npm run build && npx serve -l 3333 examples"
  },
  "dependencies": {
    "@walkeros/core": "^2.0.0",
    "@walkeros/collector": "^2.0.0",
    "@walkeros/web-core": "^2.0.0",
    "@walkeros/web-source-browser": "^2.0.0",
    "@walkeros/web-source-datalayer": "^2.0.0",
    "@walkeros/web-source-session": "^2.0.0"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0"
  },
  "repository": {
    "url": "git+https://github.com/elbwalker/walkerOS.git",
    "directory": "apps/walkerjs"
  },
  "author": "elbwalker <hello@elbwalker.com>",
  "homepage": "https://github.com/elbwalker/walkerOS#readme",
  "bugs": {
    "url": "https://github.com/elbwalker/walkerOS/issues"
  },
  "keywords": [
    "walkerOS",
    "analytics",
    "tracking",
    "data collection",
    "measurement",
    "privacy friendly",
    "web analytics",
    "walker.js"
  ]
}
