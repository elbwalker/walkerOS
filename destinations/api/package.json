{
  "name": "@elbwalker/destination-web-api",
  "version": "1.0.0",
  "description": "API web destination for walker.js",
  "main": "dist/index.js",
  "types": "dist/types",
  "scripts": {
    "build": "node ./esbuild.js && npm run copy:dts",
    "copy:dts": "copyfiles -u 1 \"src/types/*.d.ts\" dist/types",
    "dev": "jest --watchAll --colors",
    "publish": "npm i && npm run test && npm run build && npm publish --access public",
    "test": "jest --colors --coverage"
  },
  "author": "elbwalker <hello@elbwalker.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/elbwalker/walker.js",
    "directory": "destinations/api"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "dependencies": {
    "@elbwalker/walker.js": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "esbuild": "^0.17.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "bugs": {
    "url": "https://github.com/elbwalker/walker.js/issues"
  },
  "homepage": "https://github.com/elbwalker/walker.js#readme",
  "keywords": [
    "elb",
    "elbwalker",
    "walker",
    "walker.js",
