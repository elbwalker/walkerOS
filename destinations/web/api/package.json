{
  "name": "@elbwalker/destination-web-api",
  "version": "1.1.3",
  "description": "API web destination for walker.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist",
  "scripts": {
    "build": "node ./esbuild.mjs && npm run es5",
    "es5": "npm run es5:build && npm run es5:minify",
    "es5:build": "babel dist/es5.js --presets @babel/preset-env --no-babelrc -o dist/es5.js",
    "es5:minify": "uglifyjs --compress --mangle -o dist/es5.js -- dist/es5.js",
    "postbuild": "tsc && copyfiles -u 1 src/types/*.d.ts dist",
    "dev": "jest --watchAll --colors",
    "publish": "npm i && npm run test && npm run build && npm publish --access public",
    "test": "jest --colors --coverage"
  },
  "author": "elbwalker <hello@elbwalker.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/elbwalker/walker.js",
    "directory": "destinations/web/api"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.mjs",
    "dist/**/*.d.ts"
  ],
  "dependencies": {
    "@elbwalker/walker.js": "^1.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@types/jest": "^29.5.3",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.18.19",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "uglify-js": "^3.17.4"
  },
  "bugs": {
    "url": "https://github.com/elbwalker/walker.js/issues"
  },
  "homepage": "https://github.com/elbwalker/walker.js#readme",
  "keywords": [
    "elb",
    "elbwalker",
    "walker",
    "walkerjs",
    "walker destination",
    "analytics",
    "tracking",
    "api",
    "endpoint"
  ],
  "funding": [
    {
      "type": "GitHub Sponsors",
      "url": "https://github.com/sponsors/elbwalker"
    }
  ]
}
