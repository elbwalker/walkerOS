{
  "name": "@elbwalker/walkeros",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/config/*",
    "packages/clients/*",
    "packages/destinations/node/*",
    "packages/destinations/web/*",
    "packages/stacks/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=!@elbwalker/website",
    "build:website": "turbo run build --filter=@elbwalker/website",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "publish-packages": "turbo run build test && changeset version && changeset publish",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@elbwalker/jest": "*",
    "@elbwalker/tsconfig": "*",
    "@elbwalker/tsup": "*",
    "@elbwalker/types": "*",
    "@elbwalker/utils": "*",
    "eslint-config-custom": "*",
    "turbo": "^1.10.15",
    "typescript": "^5.1.6"
  },
  "packageManager": "npm@9.6.7",
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
