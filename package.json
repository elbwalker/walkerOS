{
  "name": "@elbwalker/walker.js",
  "description": "Lightweight & vendor-agnostic tracking implementation layer for data-collection by simply adding attributes to your markup.",
  "version": "1.6.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/elbwalker/walker.js"
  },
  "main": "dist/index.js",
  "types": "dist/src",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "copy:dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist/src",
    "test": "jest --colors --coverage",
    "dev": "jest --watchAll --colors",
    "build": "webpack && npm run copy:dts",
    "publish": "npm run test && npm run build && npm publish --access public"
  },
  "author": "elbwalker GmbH <hello@elbwalker.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-compat": "^4.1.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": [
    "> 0.2%",
