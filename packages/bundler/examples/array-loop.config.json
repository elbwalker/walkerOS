{
  "packages": [
    {
      "name": "@walkeros/core",
      "version": "latest"
    }
  ],
  "content": "export { getId, getByPath } from '@walkeros/core';",
  "template": {
    "file": "templates/array-loop.hbs",
    "variables": {
      "imports": [
        { "name": "getId", "package": "@walkeros/core" },
        { "name": "getByPath", "package": "@walkeros/core" }
      ],
      "configs": [
        {
          "name": "development",
          "settings": { "enabled": true },
          "version": "1.0.0"
        },
        {
          "name": "production",
          "settings": { "enabled": false },
          "version": "1.0.0"
        }
      ],
      "utils": ["getId", "getByPath", "createEvent"]
    }
  },
  "build": {
    "platform": "browser",
    "format": "esm",
    "minify": false
  },
  "output": {
    "filename": "array-loop-example.js",
    "dir": "./dist"
  }
}
