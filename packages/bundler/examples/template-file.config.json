{
  "packages": [
    {
      "name": "@walkeros/core",
      "version": "latest"
    }
  ],
  "customCode": "import { getId, getByPath } from '@walkeros/core';\nexport const generateId = () => getId(8);\nexport const extractValue = (obj, path) => getByPath(obj, path, null);",
  "template": {
    "file": "./examples/library.template.js",
    "variables": {
      "NAME": "WalkerUtils",
      "VERSION": "2.1.0",
      "BUILD_DATE": "2024-01-01"
    }
  },
  "build": {
    "platform": "browser",
    "format": "esm",
    "minify": false,
    "sourcemap": false
  },
  "output": {
    "filename": "template-file.js",
    "dir": "./dist"
  }
}
