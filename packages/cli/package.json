{
  "name": "@walkeros/cli",
  "version": "0.3.5",
  "description": "walkerOS CLI - Bundle and deploy walkerOS components",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "walkeros": "./dist/index.js"
  },
  "files": [
    "dist/**",
    "templates/**",
    "examples/**",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest --colors",
    "lint": "tsc && eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf coverage && rm -rf .temp && rm -rf .tmp",
    "docker:build": "bash scripts/build-docker.sh",
    "docker:test": "bash scripts/test-docker.sh",
    "docker:publish": "bash scripts/publish-docker.sh"
  },
  "dependencies": {
    "@walkeros/core": "^0.3.1",
    "@walkeros/docker": "*",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "esbuild": "^0.19.0",
    "fs-extra": "^11.0.0",
    "handlebars": "^4.7.8",
    "jsdom": "^22.1.0",
    "pacote": "^17.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/pacote": "^11.1.5",
    "@walkeros/config": "*",
    "tsx": "^4.0.0"
  },
  "repository": {
    "url": "git+https://github.com/elbwalker/walkerOS.git",
    "directory": "packages/cli"
  },
  "author": "elbwalker <hello@elbwalker.com>",
  "homepage": "https://github.com/elbwalker/walkerOS#readme"
}
