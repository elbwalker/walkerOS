{
  "name": "@walkeros/cli",
  "version": "1.3.0",
  "description": "walkerOS CLI - Bundle and deploy walkerOS components",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dev": {
      "types": "./dist/dev.d.ts",
      "default": "./dist/dev.js"
    }
  },
  "bin": {
    "walkeros": "./dist/cli.js"
  },
  "files": [
    "dist/**",
    "examples/**",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest --colors",
    "test:integration": "jest --colors --testPathPatterns='(cli\\.test|cli-e2e\\.test)'",
    "generate:types": "openapi-typescript ${OPENAPI_SPEC:-./openapi/spec.json} -o src/types/api.gen.d.ts",
    "generate:types:local": "OPENAPI_SPEC=${WALKEROS_APP_URL:-http://localhost:3000}/api/openapi.json npm run generate:types",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf dist && rm -rf coverage && rm -rf .temp && rm -rf .tmp",
    "docker:build": "bash scripts/build-docker.sh",
    "docker:test": "bash scripts/test-docker.sh",
    "docker:publish": "bash scripts/publish-docker.sh"
  },
  "dependencies": {
    "@walkeros/core": "1.3.0",
    "@walkeros/server-core": "1.0.5",
    "ajv": "^8.17.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "esbuild": "^0.27.1",
    "express": "^4.21.2",
    "fs-extra": "^11.3.2",
    "jsdom": "^26.1.0",
    "open": "^11.0.0",
    "openapi-fetch": "^0.17.0",
    "pacote": "^21.0.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/pacote": "^11.1.8",
    "@walkeros/config": "*",
    "msw": "^2.12.10",
    "openapi-typescript": "^7.13.0",
    "tsx": "^4.21.0"
  },
  "repository": {
    "url": "git+https://github.com/elbwalker/walkerOS.git",
    "directory": "packages/cli"
  },
  "author": "elbwalker <hello@elbwalker.com>",
  "homepage": "https://github.com/elbwalker/walkerOS#readme"
}
