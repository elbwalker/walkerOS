{
  "name": "@walkeros/cli",
  "version": "0.5.1-next.1",
  "description": "walkerOS CLI - Bundle and deploy walkerOS components",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "walkeros": "./dist/index.js"
  },
  "files": [
    "dist/**",
    "examples/**",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest --colors",
    "lint": "tsc && eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf coverage && rm -rf .temp && rm -rf .tmp",
    "docker:build": "bash scripts/build-docker.sh",
    "docker:test": "bash scripts/test-docker.sh",
    "docker:publish": "bash scripts/publish-docker.sh"
  },
  "dependencies": {
    "@walkeros/core": "^0.5.1-next.0",
    "@walkeros/docker": "0.3.1-next.0",
    "@walkeros/server-core": "0.5.1-next.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "esbuild": "^0.27.1",
    "fs-extra": "^11.3.2",
    "jsdom": "^27.3.0",
    "pacote": "^21.0.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^27.0.0",
    "@types/pacote": "^11.1.8",
    "@walkeros/config": "*",
    "tsx": "^4.21.0"
  },
  "repository": {
    "url": "git+https://github.com/elbwalker/walkerOS.git",
    "directory": "packages/cli"
  },
  "author": "elbwalker <hello@elbwalker.com>",
  "homepage": "https://github.com/elbwalker/walkerOS#readme"
}
