{{CONTENT}}

(async function() {
  const config = {
    sources: {
    {{#each sources}}
      {{@key}}: {
        code: {{{code}}},
        config: {{{config}}},
        env: {{{env}}}
      },
    {{/each}}
    },
    destinations: {
    {{#each destinations}}
      {{@key}}: {
        code: {{{code}}},
        config: {{{config}}},
        env: {{{env}}}
      },
    {{/each}}
    }{{#if collector}},
    collector: {{{collector}}}{{/if}}
  };

  return await createCollector(config);
})();