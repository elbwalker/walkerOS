{
  "name": "@elbwalker/destination-web-api",
  "description": "API web destination for walker.js",
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "npm run dev:build & npm run dev:test",
    "dev:build": "tsup --watch --config tsup.dev.config.ts",
    "dev:test": "jest --watchAll --colors",
    "lint": "eslint \"**/*.ts*\"",
    "publish": "npm run test && npm run build && npm publish --access public",
    "test": "jest --colors --coverage"
  },
  "devDependencies": {
    "@elbwalker/types": "*",
    "@elbwalker/walker.js": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elbwalker/walker.js",
    "directory": "packages/destinations/web/api"
  },
  "author": "elbwalker <hello@elbwalker.com>",
  "homepage": "https://github.com/elbwalker/walker.js#readme",
  "bugs": {
    "url": "https://github.com/elbwalker/walker.js/issues"
  },
  "keywords": [
    "elb",
    "elbwalker",
    "walker",
    "walkerjs",
    "walker destination",
    "analytics",
    "tracking",
    "api",
    "endpoint"
  ],
  "funding": [
    {
      "type": "GitHub Sponsors",
      "url": "https://github.com/sponsors/elbwalker"
    }
  ]
}
