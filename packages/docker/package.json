{
  "name": "@walkeros/docker",
  "description": "Pure runtime Docker container for walkerOS - executes pre-built flows",
  "version": "0.3.0",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "license": "MIT",
  "files": [
    "dist/**",
    "demos/**"
  ],
  "scripts": {
    "build": "tsup --silent",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.mjs",
    "lint": "tsc && eslint \"src/**/*.ts\"",
    "test": "jest",
    "docker:build": "bash scripts/build.sh",
    "docker:test": "bash scripts/build-and-test.sh",
    "docker:publish": "bash scripts/publish.sh"
  },
  "dependencies": {
    "@walkeros/core": "*",
    "cors": "^2.8.5",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node-fetch": "^2.6.13",
    "@walkeros/config": "*",
    "node-fetch": "^3.3.2",
    "tsx": "^4.21.0"
  },
  "repository": {
    "url": "git+https://github.com/elbwalker/walkerOS.git",
    "directory": "packages/docker"
  },
  "author": "elbwalker <hello@elbwalker.com>",
  "homepage": "https://github.com/elbwalker/walkerOS#readme",
  "bugs": {
    "url": "https://github.com/elbwalker/walkerOS/issues"
  },
  "keywords": [
    "walker",
    "walkerOS",
    "docker",
    "container",
    "analytics",
    "event collection",
    "tag management"
  ]
}
