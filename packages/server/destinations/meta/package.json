{
  "name": "@walkeros/server-destination-meta",
  "description": "Meta server destination for walkerOS",
  "version": "1.0.6",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dev": {
      "types": "./dist/dev.d.ts",
      "import": "./dist/dev.mjs",
      "require": "./dist/dev.js"
    },
    "./examples": {
      "types": "./dist/examples/index.d.ts",
      "import": "./dist/examples/index.mjs",
      "require": "./dist/examples/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup --silent",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "jest --watchAll --colors",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.ts*\"",
    "test": "jest",
    "update": "npx npm-check-updates -u && npm update"
  },
  "dependencies": {
    "@walkeros/core": "^1.4.0",
    "@walkeros/server-core": "^2.0.0"
  },
  "devDependencies": {
    "@walkeros/collector": "^2.0.0"
  },
  "repository": {
    "url": "git+https://github.com/elbwalker/walkerOS.git",
    "directory": "packages/server/destinations/meta"
  },
  "author": "elbwalker <hello@elbwalker.com>",
  "homepage": "https://github.com/elbwalker/walkerOS#readme",
  "bugs": {
    "url": "https://github.com/elbwalker/walkerOS/issues"
  },
  "walkerOS": {
    "type": "destination",
    "platform": "server"
  },
  "keywords": [
    "walkerOS",
    "walkerOS-destination",
    "destination",
    "server",
    "meta",
    "capi",
    "analytics"
  ],
  "funding": [
    {
      "type": "GitHub Sponsors",
      "url": "https://github.com/sponsors/elbwalker"
    }
  ]
}
