{
  "name": "@elbwalker/source-node",
  "description": "Node source for walkerOS for sending events from the server-side",
  "version": "3.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
